    "def sum_two_digits(a, b) -> float:\n",
    "    assert 0 <= a <= 9, 'a out of bounds'\n",
    "    assert 0 <= b <= 9, 'b out of bounds'\n",
    "    return a + b\n",
    "\n",
    "sum_two_digits(2, 7)"
